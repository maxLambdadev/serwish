{
  "name": "serwish",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "generate-icons": "npx svg-to-ts",
    "generate-sitemap": "node st.js",
    "build": "npm run generate-icons && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run serwish:prerender",
    "dev:ssr": "ng run serwish:serve-ssr",
    "serve:ssr": "node dist/serwish/server/main.js",
    "build:ssr": "npm run generate-icons && ng build --configuration production && ng run serwish:server:production",
    "serve": "npm run generate-icons && ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.5",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "14.2.5",
    "@angular/compiler": "14.2.5",
    "@angular/core": "14.2.5",
    "@angular/forms": "14.2.5",
    "@angular/platform-browser": "14.2.5",
    "@angular/platform-browser-dynamic": "14.2.5",
    "@angular/platform-server": "14.2.5",
    "@angular/router": "14.2.5",
    "@angular/service-worker": "14.2.5",
    "@ng-select/ng-select": "9.0.2",
    "@ngrx/effects": "14.3.2",
    "@ngrx/router-store": "14.3.2",
    "@ngrx/store": "14.3.2",
    "@nguniversal/common": "14.2.0",
    "@nguniversal/express-engine": "14.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "express": "^4.18.1",
    "express-sitemap-xml": "^3.0.1",
    "fuse.js": "^3.3.0",
    "html-to-text": "^8.1.0",
    "lodash-es": "^4.17.21",
    "ngrx-store-localstorage": "14.0.0",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-backend": "^5.0.2",
    "ngx-editor": "^13.0.0",
    "ngx-infinite-scroll": "^14.0.0",
    "ngx-mask": "^13.1.1",
    "ngx-seo": "^5.3.0",
    "ngx-sharebuttons": "^9.0.0",
    "preboot": "^8.0.0",
    "rxjs": "~7.5.7",
    "swiper": "^7.4.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.5",
    "@angular/cli": "14.2.5",
    "@angular/compiler-cli": "14.2.5",
    "@ngrx/store-devtools": "14.3.2",
    "@nguniversal/builders": "14.2.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.0",
    "@types/express-useragent": "^1.0.1",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.20.47",
    "codelyzer": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express-useragent": "^1.0.15",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.0.0",
    "install": "^0.13.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sass": "^1.43.4",
    "svg-to-ts": "^6.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "xmlhttprequest": "^1.8.0"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./src/icons/*.svg"
    ],
    "outputDirectory": "./src/icons/ts",
    "interfaceName": "Icon",
    "typeName": "swIcon",
    "prefix": "swIcon",
    "delimiter": "KEBAB",
    "optimizeForLazyLoading": false,
    "modelFileName": "icon.model",
    "compileSources": false,
    "conversionType": "files"
  },
  "browser": {
    "fs": false,
    "path": false
  }
}
